{
    "name": "kareem22t/structure-my-module",
    "description": "Laravel package for generating structured modules",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kareem22t\\StructureMyModule\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
